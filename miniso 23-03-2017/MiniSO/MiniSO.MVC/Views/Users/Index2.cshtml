@model IEnumerable<MiniSO.Entidades.User>
@{
    ViewBag.Title = "Usuario - Menu principal";
}
<p>
    @Html.ActionLink("Create New", "Create")

</p>
<table class="table">
    <tr>
        <th>@Html.DisplayNameFor(model => model.Id)</th>
        <th>@Html.DisplayNameFor(model => model.Name)</th>
        <th>@Html.DisplayNameFor(model => model.Email)</th>
        <th>@Html.DisplayNameFor(model => model.Status)</th>
        <th>Acciones Posibles</th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>@Html.DisplayFor(modelItem => item.Id)</td>
            <td>@Html.DisplayFor(modelItem => item.Name)</td>
            <td>@Html.DisplayFor(modelItem => item.Email)</td>
            <td>@Html.DisplayFor(modelItem => item.Status)</td>
            <td>
                @Html.ActionLink("Editar", "Edit", new { id = item.Id }, new { @class = "iconedit" }) |
                @Html.ActionLink("Detalles", "Details", new { id = item.Id }, new { @class = "icondetail" }) |
                @Html.ActionLink("Borrar", "Delete", new { id = item.Id }, new { @class = "icondelete" })
            </td>
        </tr>
    }

</table>
