@{
    ViewBag.Title = "RegisterNewUser";
}

<h2>RegisterNewUser</h2>
<div ng-app="app" ng-controller="UsersController">
    <form ng-submit="register_user()">
        Nick de usuario:
        <input type="text" ng-model="username"><br>
        Email:
        <input type="text" ng-model="email"><br>
        Password:
        <input type="text" ng-model="password"><br>
        <input type="submit" value="Submit">
    </form>

    <section ui-view>Response: {{mensaje}}</section>
</div>